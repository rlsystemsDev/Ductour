<% include ../header.ejs %>
<div class="wrapper">
  <% include ../navbar.ejs %>
  <% include ../sidebar.ejs %>

  <!-- Left side column. contains the logo and sidebar -->

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
          User FAQS
        <small> </small>
      </h1>

    </section>

    <!-- Main content -->
    <section class="content">

      <% if(msg!=''){%>
      <div class="alert alert-success col-xs-6" id="msg">
        <strong>Success!</strong>
        <%= msg %>
      </div>
      <%}%>
      <a href="/faq/create" class="btn btn-info pull-right" role="button">Add More</a><br></br>
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <!-- /.box-header -->
            <div class="box-body">
              <table id="example1" class="table table-bordered table-hover csv_table">
                <thead>
                  <tr>
                    <th>Sr.No</th>
                    <th>Title</th>
                    <!-- <th>Content</th> -->
                    <th>Action</th>



                  </tr>
                </thead>
                <tbody>
                  <% 
                if(response && response.length>0){
                for(var i in response){
                %>

                  <tr>
                    <td>
                      <%= parseInt(i)+1%>
                    </td>
                    <td>
                      <%= response[i].title ? response[i].title : '' %>
                    </td>
                    <!-- <td>
                      <%= response[i].content ? response[i].content : '' %>
                    </td> -->
                                    
                    <td>
                      <div>
                        <a href="/update_faqs?id=<%= response[i].id%>" class="btn btn-primary a-btn-slide-text">
                          <span class="glyphicon glyphicon-edit" aria-hidden="true"></span>
                          <span>Edit</span>
                        </a>
       
                      </div>
                      <button class="btn btn-primary btn btn-danger delete delete_cat" type="submit" rel="<%= response[i].id %>"
                        value="<%= response[i].id %>"<%= (response[i].status)? ' checked ' : '' %> table="faq"
                        >Delete</button>
                    </td>
     
                  </tr>

                  <%  }
                } %>
                </tbody>

              </table>
              <div>
                <p></p>
              </div></div> <!-- /.box-body -->
            </div>
            <!-- /.box -->


            <!-- /.box -->
          </div>
          <!-- /.col -->
        </div>
        <!-- /.row -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->



</div>

<% include ../footer.ejs %>


</div>

  
</div>

  </div>
