<% include ./header.ejs %> 
<div class="wrapper">
<% include ./navbar.ejs %> 
<% include ./sidebar.ejs %> 

  <!-- Left side column. contains the logo and sidebar -->
 
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
     Claim List Details
        <small> </small>
      </h1>
      
    </section>
    
    <!-- Main content -->
    <section class="content">
     <!-- <a href="/claimlist/create" class="btn btn-info" role="button">Add Claim</a><br></br> -->
    <% if(msg!=''){%>
      <div class="alert alert-success col-xs-6" id="msg">
        <strong>Success!</strong> <%= msg %>
      </div>
    <%}%>
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
              
            <!-- /.box-header -->
            <div class="box-body">
              <table id="example1" class="table table-bordered table-hover">
                <thead>
                <tr>
                  <th>Sr No</th>
                  <th>Hospital Name</th>
                  <th>Policy Name</th>
                  <th>DUC. ID</th>
                  <th>Username</th>
                  <!-- <th>Illness</th> -->
                  <th>No.Of Used Policies</th>
                  <th>No.Of UnUsed Policies</th>
                  <th>Claimed Amount</th>
                  <th>Claim Status</th>
                  <th>Redeem Status</th>
              <!--     <th>OTP</th> -->
                 <!--  <th>Date</th> -->
                
                </tr>
                </thead>
                <tbody>
               <% 
                if(response && response.length>0){
                for(var i in response){
                  
                %>
               
                <tr>
                  <td><%= parseInt(i)+1%></td>
                  <td><%= response[i].hospital_name? response[i].hospital_name : ''%></td>
                  <td><%= response[i].policy_name? response[i].policy_name:''%></td>
                  <td><%= response[i].policy_number? response[i].policy_number:''%></td>
                  <td><%= response[i].username? response[i].username:''%></td>
                  <td><%=(response[i].used==0)? '0' : response[i].used%></td>
                
                   <td><%=(response[i].unused==0)? '0' : response[i].unused%></td>
                 <!-- <td><%= response[i].illnesse_name? response[i].illnesse_name:''%></td> -->
                 
                   <td><%= (response[i].amount==0)? 'N/A' : response[i].amount%></td>
                   <td>
                      <% if(response[i].varify_status==0){ %>
                        <p> Not Varified</p>
                    <% } else{ %>  
                        <p>Varified</p>
                    <% } %>
                  </td>
                  
                   <td>
                    <% if(response[i].redeem_status==0){ %>
                      <p> Not Redemed</p>
                  <% } else{ %>  
                      <p>Redemeded</p>
                  <% } %>
                    
                </td>
                <!--   <td>
                    
                  </a>
                  <a href="/claimlist/claim_view?id=<%= response[i].id %>"" class="btn btn-primary a-btn-slide-text">
                      <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span>
                      <span><strong>View</strong></span>            
                  </a>
                  <button class="btn btn-primary btn btn-danger delete delete_cat" type="submit" rel="<%= response[i].id %>" value="<%= response[i].status %>" <%= (response[i].status)?'checked':'' %> table="claim_list" >Delete</button>
                  
                 </td> -->
    </tr>
                
                <%  }
                } %>
                </tbody>
                
              </table>
              <div>
                  <p></p>
                </div
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->

          
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
 

  
</div>

<% include ../footer.ejs %> 
