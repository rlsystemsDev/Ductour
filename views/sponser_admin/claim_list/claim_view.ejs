<% include ./header.ejs %> 
<div class="wrapper">
<% include ./navbar.ejs %> 
<% include ./sidebar.ejs %> 
  <div class="content-wrapper">
    


    <!-- Main content -->
    
   
    <section class="content">
        <% if(msg!=''){%>
          <div class="alert alert-error col-xs-6" id="msg">
            <strong></strong> <%= msg %>
          </div>
        <%}%>
      <div class="row">
        <!-- left column -->
        <div class="col-md-8">
          <!-- general form elements -->
          <div class="box box-primary">
            <div class="box-header with-border">
              <button type="submit" class="btn btn-info pull-right " onclick="goBack()">Back</button>
              <h3> View Claim </h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            <form role="form" method="POST" enctype="multipart/form-data" action="/claimlist/claim_save">
              <div class="box-body">
                 
              <div class="form-group">
                  <label for="exampleInputEmail1">Hospitals</label>
                  <p><%= response.h_name ? response.h_name : ''%></p>
                </div>
                <div class="form-group">
                  <label for="exampleInputEmail1">Policy Name</label>
                  <p><%= response.p_name ? response.p_name : ''%></p>
                </div>
                <div class="form-group">
                  <label for="exampleInputEmail1">Username</label>
                  <p><%= response.u_name ? response.u_name : ''%></p>
                </div>
                 <!-- <div class="form-group">
                  <label for="exampleInputEmail1">OTP</label>
                  <p><%= response.otp ? response.otp : ''%></p> -->
                <div class="form-group">
                  <label for="exampleInputEmail1">Date of Apply</label>
                  <p><%= response.date ? response.date : ''%></p>
                  
                </div>
                
                
                
                
                <input type="hidden" value="<%= response.details && response.details.id ? response.details  .id : ''  %>" name="user_id">
                
                
                
              </div>
              <!-- /.box-body -->

              
            </form>
          </div>


        </div>
        <div class="="col-md-4">
          <img id="output" style="width:300px;height:auto;max-height:500px;">
        </div>
      </div>
    </section>
    <!-- /.content -->
  </div>
  <div class="control-sidebar-bg"></div>
  
</div>
<% include ../footer.ejs %> 


