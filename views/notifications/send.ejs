<% include ../header.ejs %>
<div class="wrapper">
    <% include ../navbar.ejs %>
    <% include ../sidebar.ejs %>
    <div class="content-wrapper">



        <!-- Main content -->


        <section class="content">
            <% if(msg!=''){%>
            <div class="alert alert-success col-xs-6" id="msg">
                <strong></strong> <%= msg %>
            </div>
            <%}%>
      
      <div class="row">
        <!-- left column -->
        <div class="col-md-12">
          <!-- general form elements -->
          <div class="box box-primary">
            <div class="box-header with-border">
                <button type="submit" class="btn btn-info pull-right " onclick="goBack()">Back</button>
              <h3>Send Notification </h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            <form role="form" method="POST" enctype="multipart/form-data" action="/notifications/sendnotification">
              <div class="box-body">
                    <div class="form-group">
                        <label for="exampleInputEmail1">Notification Title</label>
                        <input type="text" name="title" class="form-control" placeholder="Enter Notification Title" required>
                    </div>
                    
                    <div class="form-group">
                        <label for="exampleInputEmail1">Notification Body</label>
                        <input type="text" name="description" class="form-control" placeholder="Enter Notification Body" required>
                    </div>
                    
                    <div class="form-group">
                        <button type="submit" class="btn btn-primary">Send</button>
                    </div>
                    
              </div>
<!-- /.box-body -->


</form>
</div>


</div>
<div class="=" col-md-4">
    <img id="output" style="width:400px;height:auto;max-height:600px;">
</div>
</div>
</section>
<!-- /.content -->
</div>
<div class="control-sidebar-bg"></div>

</div>
<% include ../footer.ejs %>